{
  "name": "rtg_vampire",
  "description": "640x360 MPEG-1 video @1250kbps, MP2 audio 128kbps",
  "output_args": [
    "-vcodec", "mpeg1video",
    "-b:v", "1250k",
    "-bf", "3",
    "-r", "24",
    "-vf", "scale=640:-2",
    "-acodec", "mp2",
    "-ar", "22050",
    "-ac", "2",
    "-af", "volume=3dB",
    "-b:a", "128k"
  ],
  "listen_port": 5001
}
