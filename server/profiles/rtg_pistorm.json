{
  "name": "rtg_pistorm",
  "description": "1280x720 MPEG-1 video @5000kbps, MP2 audio 128kbps",
  "output_args": [
    "-vcodec", "mpeg1video",
    "-b:v", "5000k",
    "-r", "25",
    "-vf", "scale=1280:-2",
    "-acodec", "mp2",
    "-ar", "22050",
    "-ac", "2",
    "-af", "volume=3dB",
    "-b:a", "128k"
  ],
  "listen_port": 5001
}
